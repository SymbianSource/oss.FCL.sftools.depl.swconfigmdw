  <data>
    <Feature1>
      <FolderSetting><localPath>default_folder</localPath></FolderSetting>
      <IntSetting>10</IntSetting>
      <StringSetting>default string</StringSetting>
      <BooleanSetting>true</BooleanSetting>
      
